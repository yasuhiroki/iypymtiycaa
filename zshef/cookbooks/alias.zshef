#!/usr/bin/env zsh

function zshef::install() {
    my::_config
}

function zshef::update() {
    my::_config
}

function my::_config() {
cat <<EOH > ~/.iypymtiycaa/aliasrc.sh
if type hub 1>/dev/null; then
    alias yukino=hub
    alias yukky=hub
    alias yuki=hub
else
    alias yukino=git
    alias yukky=git
    alias yuki=git
fi
EOH
}
