#!/usr/bin/env zsh

function zshef::install() {
    my::config
}

function zshef::update() {
    my::config
}

function my::config() {
    cat <<EOH > ~/.docker/config.json
{
  "detachKeys": "ctrl-_,_"
}
EOH
cat <<EOH > ~/.iypymtiycaa/docker.zsh
function d::sh() {
  : \${1:?Need docker container id}
  docker exec -it \${1} /bin/sh
}
EOH
}
